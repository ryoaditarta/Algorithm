#include<bits/stdc++.h>
using namespace std; 

// coin change min number of coins

int main(){
    int n; 
    cin>>n; 
    int coin[n+1]; 
    for(int i=1; i<=n; i++){
        cin>>coin[i]; 
    }
    int k; 
    cin>>k; 
    int dp[n+1][k+1]; 
    memset(dp, 0, sizeof(dp));
    for(int i=0; i<=n; i++){
       for(int j=0; j<=k; j++){
           if(i==0){
               dp[i][j] = 1e5; 
           }
       } 
    }
    
    for(int i=1; i<=n; i++){
        for(int j=1; j<=k; j++){
            if(coin[i]<=j){
                dp[i][j]=min(dp[i-1][j], dp[i][j-coin[i]]+1); 
            }else{
                dp[i][j]=dp[i-1][j]; 
            }
        }
    }
    for(int i=0; i<=n; i++){
        for(int j=0; j<=k; j++){
            cout<<dp[i][j]<<" "; 
        }
        cout<<endl; 
    }
    
    
    cout<<dp[n][k]<<endl; 
    
}
